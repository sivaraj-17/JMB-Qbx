<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="JM Bullion Shipping Automation Suite" preserve-order="true">
	<test name="JMB Shipping Upgrade Test Cases">
		<classes>
			<class name="testcases.shipping.Q_JMB_ShippingUpgrade">
				<methods>
					<exclude name="guestShippingMinDefault"/>
					<exclude name="guestDefaultShipping"/>
					<exclude name="guestShippingMinUPS3Day"/>
					<exclude name="guestShippingUpgradePri"/>
					<exclude name="guestShippingLess1K3day"/>
					<exclude name="guestShipping3DayTc2"/>
					<exclude name="regShippingLess500Pri"/>
					<exclude name="registeredMinShippingCharge"/>
					<exclude name="registeredMinShippingNoCharge"/>
					<exclude name="regLess1K3Dayupgrade"/>
					<exclude name="regOver1K3Dayupgrade"/>
					<exclude name="regOver5KOnly3Dayupgrade"/>
					<exclude name="moveBetweenShippingOptions"/>
					<exclude name="revertShippingOptions"/>
					<include name="verifyShippingOptionsLessThan$199SelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$1000AndLessThan16OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$5000AndLessThan16OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$15000AndLessThan16OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$1000AndLessThan200OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$5000AndLessThan200OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$25000AndLessThan200OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$5000AndLessThan300OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$25000AndLessThan300OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$5000AndLessThan450OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$25000AndLessThan450OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$5000AndLessThan750OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsLessThan$15000AndLessThan750OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsOver750OzSelectUPS3DayAir"/>
					<include name="verifyShippingOptionsOver$25000UPS3DayAir"/>
					<include name="verifyShippingOptionsDefaultToBaseAfterLogOff"/>
					<include name="verifyShippingOptionChangesMatchesOrderReview"/>
				</methods>
			</class>
		</classes>
	</test>

	<!-- JMB Shipping Upgrade Test Cases -->
  	
</suite>